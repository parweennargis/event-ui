<!-- partial -->
<div class="main-panel">
  <div class="content-wrapper">
    <div class="page-header">
      <h3 class="page-title"> Our Partners List </h3>
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/admin/add-partner">Create Partner</a></li>
        </ol>
      </nav>
    </div>
    <div class="row">
      <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            </p>
            <table class="table table-hover">
              <thead>
                <tr>
                  <th><b>Sl. No</b></th>
                  <th><b>Name</b></th>
                  <th><b>Image</b></th>
                  <th><b>Display</b></th>
                  <th><b>Webiste</b></th>
                  <th><b>Edit</b></th>
                  <th><b>Delete</b></th>
                </tr>
              </thead>
              <tbody>
                {{#each partners}}
                <tr>
                  <td>{{_id}}</td>
                  <td>{{name}}</td>
                  <td>{{image}}</td>
                  <td>
                    {{#if image_url}}
                      <img src="{{image_url}}" style="width: 350px; height:250px"></td>
                    {{else}}
                      <P>No Image Found!!</P>
                    {{/if}}
                    
                  <td>{{webiste}}</td>
                  <td>
                    <a href="partners/{{_id}}">
                      <div class="preview"> <i class="icon-pencil"></i> </div>
                    </a>
                  </td>
                  <td>
                    <a href="#">
                      <div class="preview sponsor-delete" data-id="{{_id}}">
                        <i class="icon-trash"></i>
                      </div>
                    </a>
                  </td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>