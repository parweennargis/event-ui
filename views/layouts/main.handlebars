<script>
    $(window).on('load', function () {
        $.ajax({
            url: '/sponsors',
            method: 'GET',
            success: function (result) {
                // console.log(result);
                var html = '';
                if (result && result.data) {
                    result.data.forEach((sponsor) => {
                        html += '<div class="item">' +
                                '   <div class="image image-wrapper">' +
                                '       <img src="' + sponsor.image_url + '" alt="Image_not_found">' +
                                '       <a href="' + sponsor.website + '" class="plus-effect" target="_blank">' +
                                '           <span>' + sponsor.title + '</span>' +
                                '       </a>' +
                                '   </div>' +
                                '</div>';
                    });
                    // console.log(html);
                    $('#partners-carousel').html(html);
                    $('#partners-carousel').owlCarousel({
                        nav: false,
                        loop: true,
                        autoplay: true,
                        smartSpeed: 1000,
                        autoplayTimeout: 2000,
                        autoplayHoverPause: true,
                        margin: 20,
                        responsive: {
                            0: {
                                items: 1,
                            },
                            360: {
                                items: 1,
                            },
                            768: {
                                items: 4,
                            },
                            991: {
                                items: 5,
                            },
                            1199: {
                                items: 7,
                            }
                        }

                    });
                }
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    })
</script>
